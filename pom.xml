<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>jp.sou4j</groupId>
  <artifactId>sou4j-podcast</artifactId>
  <packaging>jar</packaging>
  <version>1.0.0</version>
  <name>sou4j-podcast</name>
  <url>http://maven.apache.org</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.3</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <distributionManagement>
    <repository>
      <id>releases</id>
      <url>https://maven.oscalabo.net/repository/release/</url>
    </repository>
  </distributionManagement>
  <repositories>
    <repository>
      <id>oscalabo</id>
      <name>OSCA LABO Maven Repository</name>
      <url>https://maven.oscalabo.net/repository/release/</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>jp.sou4j</groupId>
      <artifactId>sou4j</artifactId>
      <version>1.0.0</version>
    </dependency>
  </dependencies>
</project>
